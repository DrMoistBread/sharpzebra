<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.9.4</Version>
    <PackageReleaseNotes>v 0.9.4: Win10 USB printer fix. Print returns bool? EPLCommands accept codepage</PackageReleaseNotes>
    <PackageId>SharpZebra.rkone</PackageId>
    <Authors>rkone</Authors>
    <Company>SharpZebra</Company>
    <Product>SharpZebra</Product>
    <Description>A .net library that simplifies printing to Zebra printers in their native EPL2/ZPL languages without needing to know EPL2 or ZPL.</Description>
    <PackageTags>Barcode Zebra Label</PackageTags>
    <Copyright>Copyright 2018</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
 
</Project>